datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Genre {
  id Int @id @default(autoincrement())
  value String @unique
  users User[]
}

model User {
  id Int @id @default(autoincrement())
  username String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  preferences Genre[]
  likedSongs Song[]
}

model Song {
  id Int @id @default(autoincrement())
  // Spotify Track ID:
  trackID String @unique
  likedBy User[]
} 


