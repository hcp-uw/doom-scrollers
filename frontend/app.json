{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "doom-scrollers",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.frontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "expo-font",
        {
          "fonts": [
            "@node_modules/@expo-google-fonts/inter/Inter-Regular.ttf",
            "@node_modules/@expo-google-fonts/inter/Inter-Bold.ttf",
            "@node_modules/@expo-google-fonts/inter/Inter-Medium.ttf",
            "@node_modules/@expo-google-fonts/inter/Inter-SemiBold.ttf",
            "@node_modules/@expo-google-fonts/inter/Inter-Light.ttf",
            "@node_modules/@expo-google-fonts/inter/Inter-Thin.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
